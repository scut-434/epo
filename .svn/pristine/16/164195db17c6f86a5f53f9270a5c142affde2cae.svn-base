body {font:12px arial,simsun,sans-serif; text-align:left; line-height:22px; margin:0; padding:0;background:#335f86;}
form,p,ol,ul,p,h1,h2,h3,h4,h5 {margin:0;padding:0; font-size:12px;}
ul {list-style:none;}
img,fieldset {border:none; display:block;}
a:link,a:visited {text-decoration:none;color:#000;}
a:hover {text-decoration:underline;color:#000;}
a:active {text-decoration:none;color:#000;}
em {font-style:normal;}
.clear {clear:both; width:100%; overflow:hidden; line-height:0; font-size:0;}
.left{float:left;display:inline;}
.right{float:right;display:inline;}
.hidden{display:none;}
