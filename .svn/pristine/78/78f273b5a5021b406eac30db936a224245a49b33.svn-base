/*页码样式*/
.page {padding:4px 6px 4px 0; font-size: 13px;  color:#313031;  font-family: microsoft yahei,Verdana,Tahoma,Arial,Helvetica,Sans-Serif; background-color: #fff; text-align:right; margin-top:20px}
.page a{padding:5px 6px 4px 5px; margin:0px 3px; color:#333; border:1px solid #ccc; text-decoration: none}
.page a:hover{border:1px solid #ccc; COLOR: #333; background:#eee}
.pagination a:active{border:1px solid #ccc; COLOR: #0066a7; background-color:#eee}
.page span.disabled {padding:5px 6px 4px 5px; margin:0px 3px; color:#999; border:1px solid #eee;}
table.pop{padding:0; margin:0; width:100%; text-align:center}