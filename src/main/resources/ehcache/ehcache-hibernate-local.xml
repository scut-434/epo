<?xml version="1.0" encoding="UTF-8"?>
<ehcache updateCheck="false">
	<diskStore path="java.io.tmpdir/hibernate/gzgb" />

	<!-- DefaultCache setting. -->
	<defaultCache maxEntriesLocalHeap="10000" eternal="false" timeToIdleSeconds="300" timeToLiveSeconds="600"
		overflowToDisk="true" memoryStoreEvictionPolicy="LRU" maxEntriesLocalDisk="100000" maxElementsInMemory="10000" />
</ehcache>
